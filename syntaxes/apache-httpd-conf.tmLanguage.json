{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "apache-httpd-conf",
	"scopeName": "source.apache-httpd-conf",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.apache-httpd-conf",
				"match": "\\b(Server(Root|Admin|Name)|(Script)?Alias|Listen|LoadModule|SSLRandomSeed|ErrorDocument|AddHandler|Include|User|Group|SetEnv|Mutex|DocumentRoot|AllowOverride|Options|DirectoryIndex|Require|((Custom)?Log(Format)?))\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.apache-httpd-conf",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.apache-httpd-conf",
					"match": "\\\\."
				}
			]
		},
		"comments": {
			"name": "comment.line.number-sign.apache-httpd-conf",
			"begin": "#",
			"end": "\n"
		}
	}
}
